{
  "manifest_version": 2,
  "name": "Midicast",
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost:8080/dist/bundle.js; object-src 'self'",
  "background": {
    "page": "index.html"
  },
  "browser_action": {
    "default_icon": "noto-piano.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "noto-piano.png"
  },
  "permissions": [
    "activeTab",
    "http://localhost/"
  ]
}
