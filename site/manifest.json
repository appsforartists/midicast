{
  "manifest_version": 2,
  "name": "MIDI Player",
  "version": "0.1",
  "content_security_policy": "script-src 'self' http://localhost:8080/dist/bundle.js; object-src 'self'",
  "background": {
    "page": "index.html"
  },
  "permissions": [
    "http://localhost/",
    "http://*/",
    "https://*/"
  ]
}
