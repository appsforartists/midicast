{
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "jsx": "react",
    "jsxFactory": "html",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6",
    "types": [
      "chrome",
      "webmidi"
    ]
  }
}
